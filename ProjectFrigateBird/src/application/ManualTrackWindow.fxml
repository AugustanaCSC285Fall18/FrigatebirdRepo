<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ManualTrackWindowController">
   <children>
      <BorderPane prefHeight="526.0" prefWidth="790.0">
         <bottom>
            <FlowPane prefHeight="100.0" prefWidth="790.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="backFiftyFramesBtn" mnemonicParsing="false" onAction="#moveBackFiftyFrames" prefHeight="100.0" prefWidth="150.0" text="Back 50 Frames" />
                  <Button fx:id="backTenFramesBtn" mnemonicParsing="false" onAction="#moveBackTenFrames" prefHeight="100.0" prefWidth="150.0" text="Back 10 Frames" />
                  <Button fx:id="forwardTenFramesBtn" mnemonicParsing="false" onAction="#moveForwardTenFrames" prefHeight="100.0" prefWidth="150.0" text="Forward 10 Frames">
                     <FlowPane.margin>
                        <Insets left="10.0" />
                     </FlowPane.margin></Button>
                  <Button fx:id="forwardFiftyFramesBtn" mnemonicParsing="false" onAction="#moveForwardFiftyFrames" prefHeight="100.0" prefWidth="150.0" text="Forward 50 Frames" />
               </children>
            </FlowPane>
         </bottom>
         <right>
            <SplitPane orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" BorderPane.alignment="CENTER">
               <items>
                  <FlowPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="setPointBtn" mnemonicParsing="false" onAction="#handleSetPointBtn" prefHeight="102.0" prefWidth="200.0" text="Set Point" />
                        <Button fx:id="undoBtn" mnemonicParsing="false" prefHeight="98.0" prefWidth="200.0" text="Undo Point" />
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </FlowPane>
               </items>
            </SplitPane>
         </right>
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="videoView" fitHeight="429.0" fitWidth="643.0" pickOnBounds="true" preserveRatio="true" />
                  <Canvas fx:id="canvas" height="431.0" onMouseClicked="#addPointForChick" width="643.0" />
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
